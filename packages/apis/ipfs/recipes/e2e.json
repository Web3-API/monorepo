[
  {
    "api": "ens/ipfs.eth",
    "constants": "./constants.json"
  },
  {
    "query": "./catFile.graphql",
    "variables": { "cid": "$SampleCid", "ipfsUrl": "$IpfsUrl" }
  },
  {
    "query": "./catFileWithOptions.graphql",
    "variables": { "cid": "$SampleCid", "ipfsUrl": "$IpfsUrl" }
  },
  {
    "query": "./addFile.graphql",
    "variables": { "data": "$File", "fileName": "$FileName", "ipfsUrl": "$IpfsUrl" }
  }
]
